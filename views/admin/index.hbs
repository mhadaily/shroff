<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

{{#each users as |user|}}
    <div class="panel panel-default">
        <div class="panel-heading">
            <a href="/users/{{user.username}}">
                {{user.name}}
            </a>
        </div>
        {{#if user.bio}}
            <div class="panel-body">{{user.bio}}</div>
        {{/if}}
    </div>
{{/each}}

